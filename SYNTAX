File ::= Statement *
Statement ::= Assignement|FunctionDefinition|ClassDefinition|FunctionCall|Expression
FunctionCall ::= IDENTIFER LPAR ExpressionList RPAR
FunctionDefinition ::= FUNCTION VARIABLE COLON ArgumentList Body
Body ::= LBRACE Statement * RBRACE
ClassDefinition ::= (FROM ClassNameList) ? CLASS VARIABLE COLON ArgumentList ClassBody
ClassBody ::= METHOD VARIABLE COLON ArgumentList Body
ArgumentList ::= (ARGUMENT (COMMA ARGUMENT) *) ?
ExpressionList ::= (Expression (COMMA Expression) *) ?
SimpleExpression ::= (STRING|FLOAT|INT|VARIABLE)
Expression ::= SimpleExpression (DOT Expression)?

